{
  "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
  "plugins": [
        "plugins/markdown"
    ],
  "opts": {
    "template": "jsdoc-template-main", // same as -t templates/default
    "encoding": "utf8", // same as -e utf8
    "destination": "./docs/", // same as -d ./out/
    "recurse": true// same as -r
  },
  "templates": {
        "referenceTitle": "KB2A BOT",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "Cài đặt kb2abot": "index.html"
        }
    },
  "footerText": "Made with ♥ by khoakomlem"
}
